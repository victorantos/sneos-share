{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/compare/*/*",
      "rewrite": "/blog-redirect.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/blog-redirect.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.css",
      "/*.json",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/*.ico",
      "/*.txt",
      "/*.xml",
      "/*.html",
      "/blog-redirect.html",
      "/ai-library*",
      "/compare*"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".css": "text/css"
  }
}