{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/ai-library",
      "redirect": "/ai-library/",
      "statusCode": 301
    },
    {
      "route": "/compare",
      "redirect": "/compare/",
      "statusCode": 301
    }
  ],
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.css",
      "/*.json",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/*.ico",
      "/*.txt",
      "/*.xml",
      "/blog-redirect.html"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".css": "text/css"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  }
}