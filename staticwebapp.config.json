{
  "trailingSlash": "always",
  "routes": [
    {
      "route": "/ai-library",
      "redirect": "/ai-library/",
      "statusCode": 301
    },
    {
      "route": "/compare",
      "redirect": "/compare/",
      "statusCode": 301
    }
  ],
  "navigationFallback": {
    "rewrite": "/blog-redirect.html",
    "exclude": ["/assets/*", "/css/*", "/js/*", "/*.png", "/*.jpg", "/*.svg", "/blog-redirect.html", "/ai-library/*", "/compare/*"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".md": "text/markdown"
  }
}