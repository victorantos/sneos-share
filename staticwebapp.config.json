{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/compare/:post.html",
      "redirect": "/compare/:post",
      "statusCode": 301
    },
    {
      "route": "/ai-library/:category/:file.html",
      "redirect": "/ai-library/:category/:file",
      "statusCode": 301
    },
    {
      "route": "/ai-library/:category/:subcategory/:file.html",
      "redirect": "/ai-library/:category/:subcategory/:file",
      "statusCode": 301
    }
  ],
  "navigationFallback": {
    "rewrite": "/blog-redirect.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.css",
      "/*.json",
      "/*.png",
      "/*.jpg",
      "/*.svg",
      "/*.ico",
      "/*.txt",
      "/*.xml",
      "/blog-redirect.html",
      "/compare/*",
      "/ai-library/*"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".css": "text/css"
  }
}